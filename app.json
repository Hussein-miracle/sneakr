{
  "expo": {
    "name": "sneakr",
    "slug": "sneakr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-image-1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splasher.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage":  "./assets/images/splash-image-1.png",
        "backgroundColor": "#011B5E"
      }
    },
    "web": {
      "favicon": "./assets/images/splash-image-1.png",
    }
  }
}
